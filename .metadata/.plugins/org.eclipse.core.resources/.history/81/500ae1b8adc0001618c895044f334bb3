
public class Application {

	public static void main(String[] args) {
		int myNumber;
		double myNumber2, myNumber3;
		myNumber= 40;
		myNumber2= 40.5;
		System.out.println("a" + myNumber);
		System.out.println("b" + myNumber);
		myNumber3= myNumber + myNumber2;
		System.out.println("c" + myNumber3);
	
		
		

	}

}
