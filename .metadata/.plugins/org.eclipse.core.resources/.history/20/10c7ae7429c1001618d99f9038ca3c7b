
public class Application {

	public static void main(String[] args) {
		
		int myNumber = 88;	
		short myShort= 847;
		long myLong= 9797;
		
		double myDouble= 7.3243;	
		
	 
		
		
		System.out.println(myNumber);
		System.out.println(myShort);
		System.out.println(myLong);
		System.out.println(myDouble);
		
		
		
		
		
		System.out.println(myNumber + myShort);
		System.out.println(myNumber - myShort); 
		System.out.println(myNumber * myShort);
		double myCreation= myNumber - myDouble; 
		double myCreation2= myCreation * myShort; 
		System.out.println(myCreation);
		System.out.println(myCreation2);
		int x = 0 + myNumber; 
		System.out.println(myNumber - x);
		 
	
			
		
	
		float myFloat= 324.3f;
		System.out.println(myFloat);
		char myChar= 'y';
		System.out.println(myChar);
		boolean myBoolean = true; 
		System.out.println(myBoolean);
		byte myByte = 127;
		System.out.println(myByte);
		
				
		

	}
	
	public int addition(int a, int b){
		
	}

}
